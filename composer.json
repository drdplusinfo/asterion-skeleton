{
  "name": "drd-plus/rules-html-skeleton",
  "license": "MIT",
  "type": "library",
  "description": "Skeleton for DrD+ rules as HTML",
  "keywords": [
    "drd plus",
    "rules",
    "helper",
    "skeleton"
  ],
  "autoload": {
    "psr-4": {
      "DrdPlus\\": "DrdPlus/",
      "Tests\\": "Tests/"
    }
  },
  "require": {
    "php": ">=7.1",
    "ext-mbstring": "*",
    "roave/security-advisories": "dev-master",
    "granam/string": "dev-master",
    "phpgt/dom": "^1.0",
    "piwik/device-detector": "^3.9",
    "tracy/tracy": "^2.4",
    "phpmailer/phpmailer": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "granam/exceptions-hierarchy": "dev-master"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
