{
  "name": "drd-plus/rules-html-skeleton",
  "license": "MIT",
  "type": "library",
  "description": "Skeleton for DrD+ rules as HTML",
  "keywords": [
    "drd plus",
    "rules",
    "helper",
    "skeleton"
  ],
  "autoload": {
    "psr-4": {
      "DrdPlus\\": "DrdPlus/",
      "Tests\\": "Tests/"
    }
  },
  "require": {
    "php": ">=7.1",
    "roave/security-advisories": "dev-master",
    "granam/string": "dev-master",
    "phpgt/dom": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
